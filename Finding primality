#include<iostream>
using namespace std;
int main(){

    int n,x;
    cout<<"Enter a number: ";
    cin>>n;


    for(int i=2; i<n/2; i++){
        if(n%i==0)
        x=0;
    }
    if(x==0)
        cout<<"The number is not prime";

    else
        cout<<"The number is prime";

        return 0;

}
